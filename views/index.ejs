<script>
$(document).ready(function() {
  $(".fancybox").fancybox({
    helpers : {
      title : {
        type : 'over'
      }
    }
  });
});


$('#projects, #web, #experience').hide();

$('.home').click(function() {
    var id = (this.title)
    var $content = $('#' + id);
    if ($('.current').length === 0) {
      showContent($content)
    }
    else {
      $('.current').fadeOut(300, function() {
        showContent($content)
      });
    }
});

function showContent(content) {
    content.fadeIn(600);
    $('.current').removeClass('current');
    content.addClass('current');
}

</script>



